<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<fragment xmlns="http://schemas.haulmont.com/cuba/screen/fragment.xsd">
    <data>
        <instance id="dc"
                  class="com.haulmont.addon.datagen.entity.str.StringPropGenSettings"
                  view="_local"/>
    </data>
    <layout>
        <hbox spacing="true">
            <lookupField id="strategyField"
                         dataContainer="dc"
                         property="strategy"
                         nullOptionVisible="true"
                         caption="msg://com.haulmont.addon.datagen.entity/StringPropGenSettings.strategy"/>
            <textField id="manualValueField" dataContainer="dc"
                       property="manualValue"
                       caption="msg://com.haulmont.addon.datagen.entity/StringPropGenSettings.manualValue"/>
            <lookupField id="fakerProviderField"
                         dataContainer="dc"
                         property="fakerFunctionRef"
                         caption="msg://com.haulmont.addon.datagen.entity/StringPropGenSettings.fakerProvider"
                         nullOptionVisible="false"/>
        </hbox>
    </layout>
</fragment>

